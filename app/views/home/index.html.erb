<div class="homepage">
  <div class='form-upload'>
    <%= render 'upload_form' %>
  </div>


  <div class="posts" >
  <div id="infinte-posts">
    <ul>
      <% @posts.each do |post| %>
        <%= render partial: 'post', locals: { post: post } %>
      <% end %>
    </ul>
  <div>
  </div>
  <%= will_paginate @posts, renderer: WillPaginateInfinite::InfinitePagination %>


</div>
